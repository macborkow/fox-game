{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [ 
      "eslint:recommended",
      "plugin:import/errors",
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "prettier/prettier": "error"
    },
    "plugins": [
      "import",
      "prettier"
    ]
}
